#extend("base")

#export("body") {
  <table class="highlight">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Records</th>
      </tr>
    </thead>
    <tbody>
      #loop(artists, "artist") {
        <tr>
          <td>#(artist.id)</td>
          <td>#(artist.name)</td>
          <td><a href="/records?artist_id=#(artist.id)">Records</a>
        </tr>
      }
    </tbody>
  </table>
}
