<!DOCTYPE html>
<html>
  <head>
    #import("head")
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.1/css/materialize.min.css">
    <link rel="stylesheet" href="/styles/index.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  </head>
  <body>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.1/js/materialize.min.js"></script>
    <script src="javascripts/index.js"></script>
    <script>
      $(document).ready(function() {
        $(".button-collapse").sideNav();
      })
    </script>
    <div class="navbar-fixed">
      <nav id="nav-normal">
        <div class="nav-wrapper">
          #raw() {
            <a href="#" data-activates="sidebar-menu" class="button-collapse show-on-large">
              <i class="material-icons">menu</i>
            </a>
          }
          <span class="brand-logo center">
            #(title)
          </span>
          #raw() {
          <ul class="right">
            <li>
              <a id="search" class="show-on-large" data-activates="search">
                <i class="material-icons">search</i>
              </a>
            </li>
          </ul>
          }
        </div>
      </nav>
      <nav id="nav-search" style="display: none;">
        <div class="nav-wrapper">
          <form id="search-form">
            <div class="input-field">
              <input id="search-field" type="search" name="contains">
              <label id="search-icon" class="label-icon" for="search"><i class="material-icons">search</i></label>
              <i id="search-close" class="material-icons" >close</i>
              </button>
            </div>
          </form>
        </div>
      </nav>
    </div>
    <ul class="side-nav" id="sidebar-menu">
      <li>
        <a href="./records">
          <i class="small material-icons">album</i>
          Records
        </a>
      </li>
      <li>
        <a href="./tracks">
          <i class="small material-icons">high_quality</i>
          Hi-Res
        </a>
      </li>
<!--
      <li>
        <a href="./genres">
          Genres
        </a>
      </li>
-->
      <li>
        <a href="./albums">
          <i class="small material-icons">library_music</i>
          Albums
        </a>
      </li>
      <li>
        <a href="./artists">
          <i class="small material-icons">assignment_ind</i>
          Artists
        </a>
      </li>
      <li>
        <a href="./owners">
          <i class="small material-icons">perm_identity</i>
          Owners
        </a>
      </li>
    </ul>
    <div class="row">
      <div class="col s10 offset-s1 center-align">
        #import("body")
      </div>
    </div>
  </body>
</html>
